    <script type="text/javascript" src="assets/scripts/services/auth-service.js"></script>
    <script type="text/javascript" src="assets/scripts/services/project-service.js"></script>
    <!-- Directives -->
    <script type="text/javascript" src="assets/scripts/directives/enter.js"></script>
