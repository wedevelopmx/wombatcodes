  </head>
  <body ng-app="geospatial" ng-controller="MainController as mainCtrl">
  	<header id="header" data-ng-include="'assets/template/header.html'"></header>

  	<section id="main">
	  	<aside id="sidebar" data-ng-include="'assets/template/sidebar.html'"></aside>
        <section class="notification" ng-class="{ active: message }" >
            <span ng-class="{danger: message.type == 'danger'}" ng-bind="message.text"></span>
        </section>
	  	<section ng-view></section>
	</section>

    <!-- content -->
  <div id="content" class="app-content box-shadow-z0" role="main">
    <div class="app-header navbar-md white box-shadow">
      <div ui-include="'../views/blocks/header.4.html'"></div>
    </div>
    <div ui-view class="app-body" ng-class="{'container': app.setting.container}" id="view"></div>
    <div class="dark dk pos-rlt" ng-class="{'hide': $state.current.data.hideFooter}">
      <div class="p-md" ng-class="{'container': app.setting.container}" >
        <div ui-include="'../views/blocks/footer.1.html'"></div>
      </div>
    </div>
  </div>
  <!-- / -->

  <!-- theme switcher -->
  <div id="switcher">
    <div ui-include="'../views/blocks/switcher.html'"></div>
  </div>
  <!-- / -->


    <!-- build:js scripts/app.angular.js -->
    <!-- jQuery -->
      <script src="libs/jquery/jquery/dist/jquery.js"></script>
    <!-- Bootstrap -->
      <script src="libs/jquery/tether/dist/js/tether.min.js"></script>
      <script src="libs/jquery/bootstrap/dist/js/bootstrap.js"></script>
      <script src="libs/jquery/PACE/pace.min.js"></script>
    <!-- Angular -->
      <script src="libs/angular/angular/angular.js"></script>
      <script src="libs/angular/angular-animate/angular-animate.js"></script>
      <script src="libs/angular/angular-route/angular-route.js"></script>
      <script src="libs/angular/angular-resource/angular-resource.js"></script>
      <script src="libs/angular/angular-sanitize/angular-sanitize.js"></script>
      <script src="libs/angular/angular-touch/angular-touch.js"></script>

      <script type="text/javascript" src="libs/angular/textAngular/dist/textAngular-rangy.min.js"></script>
      <script type="text/javascript" src="libs/angular/textAngular/dist/textAngular-sanitize.min.js"></script>
      <script type="text/javascript" src="libs/angular/textAngular/dist/textAngular.min.js"></script>

      <script type="text/javascript" src="libs/jquery/flot/jquery.flot.js"></script>
      <script type="text/javascript" src="libs/jquery/flot/jquery.flot.pie.js"></script>
      <script type="text/javascript" src="libs/jquery/flot.tooltip/js/jquery.flot.tooltip.js"></script>

      <!-- router -->
      <script src="libs/angular/angular-ui-router/release/angular-ui-router.js"></script>
      <!-- storage -->
      <script src="libs/angular/ngstorage/ngStorage.js"></script>
      <!-- utils -->
      <script src="libs/angular/angular-ui-utils/ui-utils.js"></script>
      <!-- lazyload -->
      <script src="libs/angular/oclazyload/dist/ocLazyLoad.js"></script>


    <script type="text/javascript" src="vendor/braintree-web/dist/braintree.js"></script>
