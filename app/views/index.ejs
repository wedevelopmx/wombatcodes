    <script type="text/javascript" src="assets/scripts/app.js"></script>
    <!-- Controllers -->
    <script type="text/javascript" src="assets/scripts/controllers/main-controller.js"></script>
    <script type="text/javascript" src="assets/scripts/controllers/dashboard-controller.js"></script>
    <script type="text/javascript" src="assets/scripts/controllers/profile-controller.js"></script>
